<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginTop="2dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <TextView
            android:id="@+id/tv_nombreadministrardatos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nombre" />

        <TextView
            android:id="@+id/tv_apellidoadministrardatos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Apellido" />

        <TextView
            android:id="@+id/tv_documentoadministrardatos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Documento" />

        <TextView
            android:id="@+id/tv_telefonoadministrardatos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Teléfono" />

        <TextView
            android:id="@+id/tv_correoelectronicoadministrardatos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Correo Electrónico" />

        <TextView
            android:id="@+id/tv_planillaseguridadsocialadministrardatos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Planilla Seguridad Social" />


    </LinearLayout>
</androidx.cardview.widget.CardView>
